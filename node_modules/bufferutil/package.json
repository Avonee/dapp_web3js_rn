    "node-gyp-build": "^4.2.0"
  },
  "devDependencies": {
    "mocha": "^8.0.1",
    "node-gyp": "^7.1.2",
    "prebuildify": "^4.0.0",
    "prebuildify-cross": "^4.0.0"
  },
  "react-native": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
